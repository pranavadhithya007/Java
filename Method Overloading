class father
{
    void sayHello()
    {
        System.out.println("Hello");
    }
    void sayHello(String name)
    {
        System.out.println("Hello,"+name);
    }
}
public class Main
{
    public static void main(String[]args)
    {
        father f=new father();
        f.sayHello();
        f.sayHello("Ashok");
    }
}
