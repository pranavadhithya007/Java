public class Main
{
    static int getNumber()
    {
        return 10;
    }
    public static void main(String[]args)
    {
        int num=getNumber();
        System.out.println("The number is:"+num);
    }
}

import java.util.*;
class car
{
    private String engineType="Petrol";
    public int numberOfDoor=4;
    protected String brand="Audi";
    String colour="Red";
    public car()
    {
        System.out.println("Car object created");
    }
    public String getEngineType()
    {
        return engineType;
    }
    public void setEngineType(String engineType)
    {
        this.engineType=engineType;
    }
    public void showDetails()
    {
        System.out.println("Brand: "+brand);
        System.out.println("Colour: "+colour);
        System.out.println("Door: "+numberOfDoor);
        System.out.println("Engine: "+engineType);
    }
}
public class Main
{
    public static void main(String[]args)
    {
        car myCar=new car();
        myCar.showDetails();
        myCar.setEngineType("diesel");
        System.out.println("After updating engine type:");
        myCar.showDetails();
    }
}

class student
{
    private String name;
    public String getName()
    {
        return name;
    }
    public void setName(String newName)
    {
        name=newName;
    }
}
public class Main
{
    public static void main(String[]args)
    {
        student s=new student();
        s.setName("Pranav");
        System.out.println("Student Name:"+s.getName());
    }
}

class Vehicle
{
    protected String type="Sports Vehicle";
    protected void displayType()
    {
        System.out.println("Vehicle Type:"+type);
    }
}
class Bike extends Vehicle
{
    public void showDetails()
    {
        type="Royal Enfield";
        displayType();
    }
}
public class Main
{
    public static void main(String[]args)
    {
        Bike myBike=new Bike();
        myBike.showDetails();
    }
}
